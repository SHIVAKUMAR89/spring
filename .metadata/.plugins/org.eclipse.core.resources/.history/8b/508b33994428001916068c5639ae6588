package Exception;

public class AirthmeticException {
	   public static void main(String[] args) {
	
	      try{
	         int a=30, b=5;
	         int c=a/b;
	         System.out.println ("Result: "+c);
	      }
	      catch(ArithmeticException e){
	         System.out.println ("You Shouldn't divide a number by zero");
	      }
	   }
	}
