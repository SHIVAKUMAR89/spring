import java.util.Scanner;

public class ReverseArrayString {
		    		public static void main(String args[])
		    		   {
		    		      int a[]= {1,2,3,4};
		    		      int n=0,t;    
		    		      for(int i=0;i<n/2;i++) {
		    		    	  t=a[i];
		    		    	  a[i]=a[n-i-1];
		    		    	  a[n-i-1]=t;
		    		      }
		    		      for(int k=0;k<n;k++)
		    		      {
		    		    	  System.out.println(a[k]);
		    		      }
		    		      
		    		   }
}
