package com.spring;

public class SingletonClassDemo {
	
	public static void main(String[] args) {
		
		SingletonEmployee  e= SingletonEmployee.getSt();
		
		SingletonEmployee  e1= SingletonEmployee.getSt();
	
			e.display();
			e1.display();
		
		System.out.println(e);
		
		System.out.println(e1);
		
	}

}
